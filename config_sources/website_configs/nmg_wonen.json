        "gallery_extraction_config": {
            "extra_llm_instructions": "",
            "correct_urls_paths": [
                "/huur"
            ],
            "gallery_container_selector": "#entity-results",
            "schema_type": "xpath",
            "schema": {
                "name": "NMG Wonen Huurwoningen",
                "baseSelector": "//article[@class='house huur']",
                "fields": [
                    {
                        "name": "address",
                        "selector": ".//h2",
                        "type": "text",
                        "transform": "address_extraction"
                    },
                    {
                        "name": "city",
                        "selector": ".//h2/span",
                        "type": "text"
                    },
                    {
                        "name": "rental_price",
                        "selector": ".//li[contains(@class, 'house__list-item')][1]/span[2]",
                        "type": "text",
                        "transform": "price_extraction"
                    },
                    {
                        "name": "square_meters",
                        "selector": ".//li[contains(@class, 'house__list-item')][2]/span[2]",
                        "type": "text",
                        "transform": "area_extraction"
                    },
                    {
                        "name": "bedrooms",
                        "selector": ".//li[contains(@class, 'house__list-item')][3]/span[2]",
                        "type": "text",
                        "transform": "bedroom_extraction"
                    },
                    {
                        "name": "image_url",
                        "selector": ".//img",
                        "type": "attribute",
                        "attribute": "src"
                    },
                    {
                        "name": "detail_url",
                        "selector": ".//a[@class='house__overlay']",
                        "type": "attribute",
                        "attribute": "href"
                    },
                    {
                        "name": "available_from",
                        "selector": ".//span[@class='house__rent-label']",
                        "type": "text",
                        "transform": "date_extraction"
                    },
                    {
                        "name": "is_new",
                        "selector": ".//span[@class='house__label']",
                        "type": "text"
                    }
                ]
            }
        },
        "detail_page_extraction_config": {
            "extra_llm_instructions": "",
            "schema_type": "xpath",
            "detail_container_selector": ".woning__content",
            "schema": {
                "name": "Rental Property Details",
                "baseSelector": "//div[@class='woning__content']",
                "fields": [
                    {
                        "name": "description",
                        "selector": ".//div[@class='woning__description js--has-toggler']/div[@class='js--toggler__target woning__prose']",
                        "type": "text"
                    },
                    {
                        "name": "rental_price",
                        "selector": ".//div[@class='woning__features']//dt[contains(text(), 'Huurprijs')]/following-sibling::dd[1]",
                        "type": "text",
                        "transform": "strip"
                    },
                    {
                        "name": "status",
                        "selector": ".//div[@class='woning__features']//dt[contains(text(), 'Status')]/following-sibling::dd[1]",
                        "type": "text",
                        "transform": "strip"
                    },
                    {
                        "name": "available_from",
                        "selector": ".//div[@class='woning__features']//dt[contains(text(), 'Aanvaarding datum')]/following-sibling::dd[1]",
                        "type": "text",
                        "transform": "strip"
                    },
                    {
                        "name": "square_meters",
                        "selector": ".//div[@class='woning__features']//dt[contains(text(), 'Woonoppervlakte')]/following-sibling::dd[1]",
                        "type": "text",
                        "transform": "strip"
                    },
                    {
                        "name": "year_of_construction",
                        "selector": ".//div[@class='woning__features']//dt[contains(text(), 'Bouwjaar')]/following-sibling::dd[1]",
                        "type": "text",
                        "transform": "strip"
                    },
                    {
                        "name": "number_of_rooms",
                        "selector": ".//div[@class='woning__features']//dt[contains(text(), 'Aantal kamers')]/following-sibling::dd[1]",
                        "type": "text",
                        "transform": "strip"
                    },
                    {
                        "name": "number_of_bedrooms",
                        "selector": ".//div[@class='woning__features']//dt[contains(text(), 'Aantal slaapkamers')]/following-sibling::dd[1]",
                        "type": "text",
                        "transform": "strip"
                    },
                    {
                        "name": "service_costs",
                        "selector": ".//div[@class='woning__description js--has-toggler']/div[@class='js--toggler__target woning__prose']/p[contains(text(), 'Servicekosten')]",
                        "type": "regex",
                        "pattern": "â‚¬\\s*([\\d\\.,]+)\\s*per maand"
                    }
                ]
            }
        }
    }
